{
  "name": "qa-intelligence-elysia",
  "version": "1.0.0",
  "description": "Elysia-powered microservices for QA Intelligence automation testing platform",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun run src/server.ts",
    "dev": "bun --watch src/server.ts",
    "build": "bun build src/server.ts --outdir ./dist --target bun",
    "test": "bun test"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.3.3",
    "@elysiajs/jwt": "^1.3.3",
    "@elysiajs/swagger": "^1.3.1",
    "elysia": "^1.3.21",
    "openai": "^5.19.1"
  },
  "keywords": [
    "elysia",
    "bun",
    "qa",
    "testing",
    "automation",
    "typescript",
    "microservices"
  ]
}
