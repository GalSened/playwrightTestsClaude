		{
			"name": "API 1: Dashboard - Primary Endpoint",
			"item": [
				{
					"name": "GET Dashboard Data - Success",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Test 1: Status code is 200",
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"// Test 2: Response time is less than 1000ms",
									"pm.test(\"Response time is less than 1000ms\", function () {",
									"    pm.expect(pm.response.responseTime).to.be.below(1000);",
									"});",
									"",
									"// Test 3: Response has correct structure",
									"pm.test(\"Response has summary object\", function () {",
									"    const jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('summary');",
									"});",
									"",
									"// Test 4: Summary has required fields",
									"pm.test(\"Summary has all required fields\", function () {",
									"    const summary = pm.response.json().summary;",
									"    pm.expect(summary).to.have.property('totalTests');",
									"    pm.expect(summary).to.have.property('totalRuns');",
									"    pm.expect(summary).to.have.property('successRate');",
									"    pm.expect(summary).to.have.property('averageDuration');",
									"    pm.expect(summary).to.have.property('testCoverage');",
									"    pm.expect(summary).to.have.property('healthScore');",
									"});",
									"",
									"// Test 5: Data types are correct",
									"pm.test(\"Summary fields have correct data types\", function () {",
									"    const summary = pm.response.json().summary;",
									"    pm.expect(summary.totalTests).to.be.a('number');",
									"    pm.expect(summary.totalRuns).to.be.a('number');",
									"    pm.expect(summary.successRate).to.be.a('number');",
									"    pm.expect(summary.averageDuration).to.be.a('number');",
									"    pm.expect(summary.healthScore).to.be.a('number');",
									"});",
									"",
									"// Test 6: Values are in valid ranges",
									"pm.test(\"Values are in valid ranges\", function () {",
									"    const summary = pm.response.json().summary;",
									"    pm.expect(summary.successRate).to.be.within(0, 100);",
									"    pm.expect(summary.healthScore).to.be.within(0, 100);",
									"    pm.expect(summary.totalTests).to.be.at.least(0);",
									"});",
									"",
									"// Test 7: Categories array exists",
									"pm.test(\"Response has categories array\", function () {",
									"    const jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('categories');",
									"    pm.expect(jsonData.categories).to.be.an('array');",
									"});",
									"",
									"// Test 8: Trends array exists",
									"pm.test(\"Response has trends array\", function () {",
									"    const jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('trends');",
									"    pm.expect(jsonData.trends).to.be.an('array');",
									"});",
									"",
									"// Test 9: AI Insights exists",
									"pm.test(\"Response has aiInsights object\", function () {",
									"    const jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('aiInsights');",
									"});",
									"",
									"// Store values for next tests",
									"pm.environment.set(\"dashboard_totalTests\", pm.response.json().summary.totalTests);",
									"pm.environment.set(\"dashboard_healthScore\", pm.response.json().summary.healthScore);"
								],
								"type": "text/javascript"
							}
						}
					],
