# Backend Production Environment Variables
NODE_ENV=production
PORT=3002

# Database Configuration
DATABASE_URL=postgresql://playwright_user:changeme_secure_password@postgres:5432/playwright_enterprise_prod

# Redis Configuration
REDIS_URL=redis://redis:6379

# Authentication & Security
JWT_SECRET=changeme_very_secure_jwt_secret_minimum_32_chars
SESSION_SECRET=changeme_secure_session_secret

# External Services
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=changeme_supabase_service_key

# Feature Flags
ENABLE_METRICS=true
ENABLE_BACKGROUND_JOBS=true
MAX_CONCURRENT_JOBS=20

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# Performance
MAX_PAYLOAD_SIZE=50mb
REQUEST_TIMEOUT=30000

# Security
CORS_ORIGIN=http://localhost
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100