{
  "name": "qa-intelligence-backend",
  "version": "2.0.0",
  "description": "Enterprise Backend for QA Intelligence Test Management Platform",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:enterprise": "tsx watch src/server-enterprise.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:enterprise": "node dist/server-enterprise.js",
    "test": "node scripts/test-runner.js",
    "test:unit": "node scripts/test-runner.js --type unit",
    "test:integration": "node scripts/test-runner.js --type integration",
    "test:performance": "node scripts/test-runner.js --type performance",
    "test:watch": "node scripts/test-runner.js --watch",
    "test:coverage": "node scripts/test-runner.js --coverage",
    "test:ci": "node scripts/test-runner.js --env ci --coverage --bail",
    "test:db:setup": "node scripts/test-db-setup.js setup",
    "test:db:cleanup": "node scripts/test-db-setup.js cleanup",
    "test:db:check": "node scripts/test-db-setup.js check",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "migrate": "tsx src/database/migrate.ts",
    "seed": "tsx src/database/seed.ts",
    "sync:db": "tsx src/scripts/syncTestDatabase.ts",
    "validate:db": "tsx src/scripts/validateDatabaseIntegrity.ts",
    "healthcheck:api": "tsx src/scripts/apiHealthCheck.ts",
    "worker": "tsx src/workers/scheduler.ts",
    "ingest:docs": "tsx src/scripts/ingestKnowledge.ts",
    "docker:up": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:logs": "docker-compose -f docker-compose.dev.yml logs -f"
  },
  "keywords": [
    "playwright",
    "testing",
    "enterprise",
    "saas",
    "multi-tenant",
    "monitoring",
    "analytics"
  ],
  "author": "Enterprise Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.67.0",
    "@langchain/community": "^0.3.53",
    "@langchain/core": "^0.3.72",
    "@langchain/openai": "^0.6.9",
    "@pinecone-database/pinecone": "^6.1.2",
    "@supabase/supabase-js": "^2.56.0",
    "@types/bcrypt": "^6.0.0",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/newman": "^5.3.6",
    "@types/pg": "^8.15.5",
    "@types/socket.io": "^3.0.1",
    "@types/xml2js": "^0.4.14",
    "@types/yauzl": "^2.10.3",
    "@xenova/transformers": "^2.17.2",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^9.2.2",
    "bull": "^4.16.5",
    "bullmq": "^5.58.2",
    "chokidar": "^4.0.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.1",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "faiss-node": "^0.5.1",
    "fluent-ffmpeg": "^2.1.3",
    "helmet": "^7.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.31",
    "mammoth": "^1.10.0",
    "multer": "^2.0.2",
    "newman": "^6.2.1",
    "newman-reporter-html": "^1.0.5",
    "newman-reporter-json": "^1.0.0",
    "node-cron": "^3.0.3",
    "onnxruntime-web": "^1.22.0",
    "openai": "^5.16.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "sharp": "^0.34.3",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.6",
    "tesseract.js": "^5.0.5",
    "tiktoken": "^1.0.22",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "xml2js": "^0.6.2",
    "yaml": "^2.8.1",
    "yauzl": "^3.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
